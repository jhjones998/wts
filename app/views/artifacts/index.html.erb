<p id="notice"><%= notice %></p>

<h1>Artifacts</h1>

<table>
  <thead>
    <tr>
      <th>Country</th>
      <th>L1</th>
      <th>L2</th>
      <th>L3</th>
      <th>L4</th>
      <th>L5</th>
      <th>L6</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artifacts.each do |artifact| %>
      <tr>
        <td><%= artifact.country.name %></td>
        <td><%= artifact.l1 %></td>
        <td><%= artifact.l2 %></td>
        <td><%= artifact.l3 %></td>
        <td><%= artifact.l4 %></td>
        <td><%= artifact.l5 %></td>
        <td><%= artifact.l6 %></td>
        <td><%= link_to 'Show', artifact %></td>
        <td><%= link_to 'Edit', edit_artifact_path(artifact) %></td>
        <td><%= link_to 'Destroy', artifact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artifact', new_artifact_path %>
