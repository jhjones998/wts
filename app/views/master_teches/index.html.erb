<p id="notice"><%= notice %></p>

<h1>Master Teches</h1>

<table>
  <thead>
    <tr>
      <th>Wtd</th>
      <th>Level</th>
      <th>Name</th>
      <th>Desc</th>
      <th>Unresearched desc</th>
      <th>Initially visible</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @master_teches.each do |master_tech| %>
      <tr>
        <td><%= master_tech.wts_id %></td>
        <td><%= master_tech.level %></td>
        <td><%= master_tech.name %></td>
        <td><%= master_tech.desc %></td>
        <td><%= master_tech.unresearched_desc %></td>
        <td><%= master_tech.initially_visible %></td>
        <td><%= link_to 'Show', master_tech %></td>
        <td><%= link_to 'Edit', edit_master_tech_path(master_tech) %></td>
        <td><%= link_to 'Destroy', master_tech, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Master Tech', new_master_tech_path %>
